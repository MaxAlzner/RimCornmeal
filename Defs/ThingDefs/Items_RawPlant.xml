<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="RoughPlantBase">
    <defName>XM_Cornmeal</defName>
    <label>cornmeal</label>
    <description>Cornmeal, used in baking. Cornmeal takes a very long time to go bad, if stored in a dry environment. It was produced by corn that has been milled into a powder.</description>
    <soundInteract>Grain_Drop</soundInteract>
    <soundDrop>Grain_Drop</soundDrop>

    <graphicData>

      <texPath>Things/Items/Raw/Cornmeal</texPath>

    </graphicData>
    <statBases>
      <MarketValue>1.9</MarketValue>
      <Nutrition>0.05</Nutrition>
      <Flammability>1.3</Flammability>
    </statBases>
    <thingCategories>
      <li>PlantMatter</li>
    </thingCategories>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>100</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>

    <ingestible>
      <preferability>NeverForNutrition</preferability>
    </ingestible>
  </ThingDef>
  
</Defs>